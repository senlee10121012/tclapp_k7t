diff --git a/source/Kconfig.melis b/source/Kconfig.melis
index bcfcc37..2e1a7f5 100755
--- a/source/Kconfig.melis
+++ b/source/Kconfig.melis
@@ -76,19 +76,33 @@ config CC_STACKPROTECTOR_STRONG
 	bool "enable stack-protector"
 	default y
 	help
-	enable stack-proctor
+		Enable stack-proctor
 
 config LOG_RELEASE
 	bool "Log release option"
 	default n
 	help
-	Reduce log
+		Reduce log
+
+config BOOTUP_TURBO
+	bool "bootup turbost"
+	default n
+	select DISABLE_ALL_PRINTF
+	help
+		fast bootup
+
+config DISABLE_ALL_PRINTF
+	bool "disable printf and rt_printf"
+	depends on BOOTUP_TURBO
+	help
+		Reduce all the log.
+
 config FRAME_POINTER  
 	bool "enable FP register as frame pointer"
 	default y 
 	select CC_OPTIMIZE_FOR_DEBUG
 	help
-	frame pointer enabled!
+		frame pointer enabled!
 
 choice
 	prompt "Compiler optimization level"
