+static int ntfs_link(ntfs_inode *ni, ntfs_inode *dir_ni, const ntfschar *name,
+                     u8 name_len)
+{
+    return (ntfs_link_i(ni, dir_ni, name, name_len, FILE_NAME_POSIX));
+}
