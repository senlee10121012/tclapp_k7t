+    blit.src_image_h.gamut = G2D_BT601;
+    blit.src_image_h.bpremul = 0;
+    //blit.src_image_h.alpha = 0xff;
+    blit.src_image_h.mode = G2D_PIXEL_ALPHA;   //G2D_PIXEL_ALPHA, G2D_GLOBAL_ALPHA
